{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "jinja",
      "camelcase": "Jinja",
      "title": "Jinja2",
      "scope": "source.jinja",
      "file-types": [
        "jinja2",
        "jinja",
        "j2"
      ],
      "injection-regex": "^jinja$",
      "class-name": "TreeSitterJinja"
    }
  ],
  "metadata": {
    "version": "0.1.4",
    "license": "MIT",
    "description": "Jinja2 grammar for the tree-sitter parsing library",
    "authors": [
      {
        "name": "Zach Kanzler",
        "email": "they4kman@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/theY4Kman/tree-sitter-jinja"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}
