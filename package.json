{
  "name": "tree-sitter-jinja",
  "version": "0.3.1",
  "description": "Jinja2 grammar for tree-sitter",
  "dependencies": {
    "nan": "^2.17.0",
    "node-addon-api": "^8.3.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.25.1"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "tree-sitter": [
    {
      "scope": "source.jinja",
      "file-types": [
        "j2",
        "jinja2",
        "jinja"
      ]
    }
  ]
}
